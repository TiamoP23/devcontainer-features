{
    "id": "postgresql",
    "version": "1.0.0",
    "name": "TiamoP23 PostgreSQL",
    "options": {
        "version": {
            "type": "string",
            "proposals": ["latest", "15", "14", "13", "12", "11"],
            "default": "latest",
            "description": "Select or enter a version of PostgreSQL."
        }
    },
    "containerEnv": {
        "DATABASE_URL": "postgres://postgres@localhost:5432/postgres"
    },
    "dependsOn": {
        "ghcr.io/itsmechlark/features/postgresql": {
            "version": "${VERSION}"
        }
    }
}
